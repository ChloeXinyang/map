{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "layer": [
      {
        "data": {
          "url": "australian-states.json",
          "format": {
            "type": "json",
            "property": "features"
          }
        },
        "projection": {
          "type": "mercator",
          "scale": 800,
          "center": [133, -28]
        },
        "mark": {
          "type": "geoshape",
          "fill": "#e0e0e0",
          "stroke": "black",
          "strokeWidth": 1
        }
      },
      {
        "data": {
          "url": "National_Heritage_List_Spatial_Database_(NHL)_-_public.json",
          "format": {
            "type": "topojson",
            "feature": "arcs"
          }
        },
        "mark": {
          "type": "circle",
          "color": "#e34a33",
          "tooltip": true
        },
        "encoding": {
          "longitude": {"field": "properties.longitude", "type": "quantitative"},
          "latitude": {"field": "properties.latitude", "type": "quantitative"},
          "size": {
            "field": "properties.number_of_sites",
            "type": "quantitative",
            "scale": {"domain": [1, 100], "range": [10, 1000]},
            "legend": {"title": "Number of Heritage Sites"}
          }
        }
      }
    ]
  }
  